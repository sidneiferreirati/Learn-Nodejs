<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      * {
        list-style: none;
        margin: 0;
        padding: 0;
        font-family: "Fira code";
      }
      #usuarios {
        border: 1px solid black;
        width: 300px;
        padding: 15px 5px;
        margin: 10px 5px;
        border-radius: 5px;
      }
    </style>
  </head>
  <body>
    <!-- <% const usuario = {} %>  -->
    <% if(users.length > 0){ %>
    <h2>Usuarios Cadastrados no banco de dados</h2>
    <% users.forEach(usuario => { %>
    <ul id="usuarios">
      <li>Nome : <%= usuario.nome %></li>
      <li>Sobrenome: <%= usuario.sobrenome %></li>
      <li>Email: <%= usuario.email %></li>
      <li>Senha: <%= usuario.password %></li>
    </ul>
    <% }); %> <% }else{ %>
    <h2>Nenhum usuarios cadastrados no banco de dados</h2>
    <% } %>
  </body>
</html>
